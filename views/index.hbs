    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f4f4f4;
            margin: 0;
            padding: 0;
        }

        header {
            background-color: #d82612;
            color: #fff;
            padding: 20px;
            text-align: center;
            position: relative;
            overflow: hidden;
        }

        .header-shape {
            width: 100%;
            height: 150px;
            position: absolute;
            bottom: -40px;
            left: 0;
            z-index: -1;
            background: #4CAF50;
            clip-path: polygon(0% 0%, 100% 0%, 100% 85%, 0% 100%);
        }

        h1 {
            margin: 0;
        }

        .container {
            max-width: 800px;
            margin: 20px auto;
            padding: 20px;
            background-color: #fff;
            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
            border-radius: 5px;
            position: relative;
            /* Added */
        }

        ul {
            list-style-type: none;
            padding: 0;
        }

        li {
            margin: 10px 0;
            display: flex;
            justify-content: space-between;
            align-items: center;
            border-bottom: 1px solid #ccc;
            padding-bottom: 10px;
        }

        .btn-download,
        .btn-preview {
            padding: 8px 16px;
            background-color: #007BFF;
            color: #fff;
            border: none;
            border-radius: 5px;
            cursor: pointer;
            text-decoration: none;
        }

        .btn-preview {
            background-color: #28a745;
        }

        .btn-download:hover,
        .btn-preview:hover {
            background-color: #0056b3;
        }

        .year {
            font-weight: bold;
            font-size: 1.2rem;
        }

        /* Styling for left and right ads */
        /* Removed .left-ad and .right-ad styles */

        @keyframes slideIn {
            from {
                opacity: 0;
                transform: translateY(-20px);
            }

            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        @keyframes fadeIn {
            from {
                opacity: 0;
            }

            to {
                opacity: 1;
            }
        }

        /* Apply animations to header and buttons */
        header {
            animation: fadeIn 1s;
        }

        .btn-download,
        .btn-preview {
            animation: slideIn 0.5s ease-in-out;
        }

        /* Add hover animations for buttons */
        .btn-download:hover,
        .btn-preview:hover {
            transform: scale(1.05);
        }

        /* Removed .left-ad and .right-ad animations */

        @keyframes walk {
            0% {
                left: 100px;
                /* Start from the left */
            }

            100% {
                left: calc(100% - 1180px);
                /* End at the start of h1 */
            }
        }

        /* Style for the walking character */
        .walking-character {
            position: absolute;
            bottom: 20px;
            /* Adjust the position as needed */
            animation: walk 10s linear infinite;
            /* Adjust duration and timing as needed */
        }




        /* Style for right ad */
        .right-ad {
            position: absolute;
            top: 90px;
            right: 0;
            width: 20%;
            /* Adjust the width as needed */
            padding: 10px;
        }

        .left-ad {
            position: absolute;
            top: 90px;
            left: 0;
            width: 20%;
            /* Adjust the width as needed */
            padding: 10px;
        }

        /* Adjust the positioning and styling as needed */

        /* Style for the logo */
        .logo {
            float: left;
            height: 40px;
            width: 50px;
            top: 0;
        }









        .additional-container {

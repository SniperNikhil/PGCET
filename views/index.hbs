        .channel-details p {
            margin: 10px 0;
            font-size: 16px;
            line-height: 1.4;
        }

        .channel-details strong {
            color: #333;
        }

        .channel-details a {
            text-decoration: none;
            color: #0078e7;
            font-weight: bold;
        }

        .channel-details a:hover {
            text-decoration: underline;
        }



        .thank-you-message {
            background-color: #eb0aff;
